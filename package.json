{
	"name": "mashareprintseva.com-sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . && prettier --check .",
		"format": "prettier --write .",
		"images:convert": "bun run ./utils/convert.js ",
		"prepare": "husky"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "5.4.7",
		"@sveltejs/kit": "2.8.0",
		"@sveltejs/vite-plugin-svelte": "4.0.0",
		"@types/eslint": "9.6.1",
		"eslint": "9.14.0",
		"eslint-config-prettier": "^1.0.3",
		"eslint-plugin-svelte": "2.46.0",
		"globals": "15.12.0",
		"husky": "9.1.7",
		"mdsvex": "0.11.2",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.2.7",
		"sharp": "0.33.5",
		"svelte": "5.1.13",
		"svelte-check": "4.0.6",
		"typescript": "5.6.3",
		"typescript-eslint": "8.13.0",
		"vite": "5.4.10"
	},
	"type": "module"
}
